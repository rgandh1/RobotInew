<diameter>

  <header> 
    <!-- These lines have no effect since they will be created (or replaced) by JDiameter automatically
    <version>1</version>                   The resulting request will again have the JDiameter generated version=1   
    <applicationId>4</applicationId>       The resulting request will get the constant Credit Control Application Id=4  
    <hopByHopId>0</hopByHopId>             The resulting request will have JDiameter generated hopByHopId that is unique within all rests of the session (can be used for correlation of incomming responses)      
    <endToEndId>1682964480</endToEndId>    The resulting request will have JDiameter generated endToEndId that is (at least for 4min) unique within all requests of the connection  
    -->  
    <commandCode>272</commandCode>         <!-- 272 = CC = CreditControl-->
    <flags>  
      <isRequest>true</isRequest>          <!-- true = R = Request; false = A = Answer-->
      <isProxiable>false</isProxiable>  
      <isError>false</isError>  
      <isReTransmitted>false</isReTransmitted>  
    </flags>  
  </header>  

  <avps>  
    <!-- These lines have no effect since they will be created (or replaced) by JDiameter automatically
    <Session-Id>ocs01.inew.com;323;21885584</Session-Id>     The resulting request will have the Session-Id defined on session creation (this AVP must always be located BEFORE all other AVPs!!!) 
    <Auth-Application-Id>5</Auth-Application-Id>             ... will get the constant Credit Control Application Id=4  
    <Destination-Realm>testing.inew</Destination-Realm>      ... will get the Destination-Realm configured in WorkspaceOptions dialog
    <Destination-Host>10.105.140.159</Destination-Host>      ... will get the Destination-Host configured in WorkspaceOptions dialog
    <Origin-Host>ocs01.inew.com</Origin-Host>                ... will get the JDiameter generated Origin-Host configured in WorkspaceOptions dialog
    <Origin-Realm>inew.com</Origin-Realm>                    ... will get the JDiameter generated Origin-Realm configured in WorkspaceOptions dialog
    -->
    <User-Name>@CG: callSubscriberMsisdn()</User-Name>  <!-- @CG: callSubscriberMsisdn() -->
    
    <CC-Request-Type>1</CC-Request-Type>      <!-- 1=INITIAL_REQUEST 2=UPDATE_REQUEST 3=TERMINATION_REQUEST -->
    <CC-Request-Number>0</CC-Request-Number>  <!-- request index 0 -->
    <CC-Correlation-Id>1234567</CC-Correlation-Id> 
    
    <Event-Timestamp>2084-07-17T11:19:28.000+0200</Event-Timestamp> <!-- RFC 822 Timestamp - e.g. 2014-07-17T11:19:28.000+0200 @CG: currentTime(+00:00:00) -->
 
    <Subscription-Id>
      <Subscription-Id-Type>0</Subscription-Id-Type>  			<!-- 0=END_USER_E164 1=END_USER_IMSI 2=END_USER_SIP_URI 3=END_USER_NAI 4=END_USER_PRIVATE -->
      <Subscription-Id-Data>@CG: callSubscriberMsisdn()</Subscription-Id-Data> 
    </Subscription-Id> 
