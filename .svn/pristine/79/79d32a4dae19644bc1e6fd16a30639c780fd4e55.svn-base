CloseLog
    Open Connection    ${SMPPGWSERVERIP}    Port=22    prompt=$
    login    ${SMPPUSER}    ${SMPPPASSWORD}
    Write    sleep 10
    Write    kill -9 `ps -ef | grep "tail -f smppgw.log | tee" | grep -v grep | awk {'print $2'}`
    Close Connection

Download log
    Open Connection    ${SMPPGWSERVERIP}    Port=22    prompt=$
    login    ${SMPPUSER}    ${SMPPPASSWORD}
    Write    cd ${MSGLOGPATH}
    Get File    ${MSGLOGPATH}/smppgw_stresstest.log    ${DOWNLOADTO}
    Close Connection
